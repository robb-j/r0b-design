<div class="skeleton-group">
  <h2>about section</h2>
  
  <div class="skeleton-example with-light-box">
    <div class="about-me">
      <img class="profile-pic" width="128" height="128" src="https://picsum.photos/id/512/512/512">
      <p class="strapline"> I'm making ... stuff </p>
      <div class="links">
        <a class="button is-circle is-twitter">
          {{{ @data.assets.fa.twitter-brands }}}
        </a>
        <a class="button is-circle is-github">
          {{{ @data.assets.fa.github-brands }}}
        </a>
        <a class="button is-circle is-dockerhub">
          {{{ @data.assets.fa.docker-brands }}}
        </a>
        <a class="button is-circle is-instagram">
          {{{ @data.assets.fa.instagram-brands }}}
        </a>
        <a class="button is-circle is-scholar">
          {{{ @data.assets.fa.graduation-cap-solid }}}
        </a>
      </div>
    </div>
  </div>
</div>

<div class="skeleton-group">
  <h2>cards</h2>
  
