  <div class="skeleton-example with-shadow is-smallish">
    <div class="clean-card">
      <h3 class="card-title">Latest Projects</h3>
      <div class="card-content">
        <p>{{ @data.text.paragraph }}</p>
      </div>
    </div>
  </div>
  
  <h3>project</h3>
  <div class="skeleton-example is-smallish">
    <div class="project-card">
      <a class="card-cover-image" href="#">
        <div style="background-image: url(https://picsum.photos/id/512/512/512)">
        </div>
      </a>
      <div class="card-content">
        <p>
          <a href="#">Spontaneous project alpha</a>
        </p>
        <div class="tags is-right">
          <span class="tag is-success">Tag A</span>
          <span class="tag is-warning">Tag B</span>
          <span class="tag is-link">Tag C</span>
        </div>
      </div>
    </div>
  </div>
</div>

